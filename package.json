    "format:fix": "prettier --write .",
    "lint:check": "eslint .",
    "lint:fix": "eslint . --fix",
    "pre:install": "bun update --latest --recursive && bun install",
    "prepare": "bun run build",
    "prepare:deploy": "bun run format:fix && bun run lint:fix && bun run build",
