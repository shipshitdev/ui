    "react-day-picker": "9.13.0",
    "sonner": "2.0.7",
    "tailwind-merge": "3.4.0"
  },
  "description": "A modern, accessible React component library built with TypeScript, Tailwind CSS v4, and Radix UI. Features primitives, composites, and patterns with full theme support and Storybook documentation.",
  "devDependencies": {
    "@biomejs/biome": "2.3.11",
    "@happy-dom/global-registrator": "20.1.0",
    "@storybook/addon-a11y": "10.1.11",
    "@storybook/addon-docs": "10.1.11",
    "@storybook/addon-links": "10.1.11",
    "@storybook/addon-vitest": "10.1.11",
    "@storybook/react": "10.1.11",
    "@storybook/react-vite": "10.1.11",
    "@tailwindcss/postcss": "4.1.18",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "16.3.1",
    "@types/bun": "1.3.5",
    "@types/node": "25.0.3",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "@vitejs/plugin-react": "5.1.2",
    "@vitest/browser-playwright": "4.0.16",
    "@vitest/coverage-v8": "4.0.16",
    "ajv": "8.17.1",
    "autoprefixer": "10.4.23",
    "husky": "9.1.7",
    "lint-staged": "16.2.7",
    "playwright": "1.57.0",
    "postcss": "8.5.6",
    "postcss-cli": "11.0.1",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "sass": "1.97.2",
    "storybook": "10.1.11",
    "tailwindcss": "4.1.18",
    "typescript": "5.9.3",
    "vite": "7.3.1",
    "vite-plugin-dts": "4.5.4",
    "vitest": "4.0.16",
    "vitest-axe": "0.1.0"
  },
  "exports": {
    ".": {
      "import": {
        "default": "./dist/index.js"
      },
      "require": {
        "default": "./dist/index.cjs"
      }
    },
    "./styles.css": "./dist/styles.css",
    "./tailwind.preset": "./dist/tailwind.preset.js",
    "./themes/*": "./dist/themes/*.scss"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "react",
    "components",
    "ui",
    "tailwind",
    "shipshitdev"
  ],
  "license": "MIT",
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,md,css,scss}": [
      "biome check --write --no-errors-on-unmatched"
    ]
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "name": "@shipshitdev/ui",
  "packageManager": "bun@1.1.14",
  "peerDependencies": {
    "@tailwindcss/postcss": ">=4.0.0 <5",
    "react": "18.0.0 || ^19.0.0",
    "react-dom": "18.0.0 || ^19.0.0",
    "tailwindcss": ">=4.0.0 <5"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shipshitdev/ui"
  },
  "scripts": {
    "build": "bun run build:lib && bun run build:css && bun run build:themes && bun run build:preset",
    "build-storybook": "storybook build",
    "build:css": "bunx postcss ./src/styles/globals.scss -o ./dist/styles.css --verbose",
    "build:lib": "./scripts/build-lib.sh",
    "build:preset": "node scripts/build-preset.mjs",
    "build:themes": "node scripts/build-themes.mjs",
    "check": "biome check .",
    "check:fix": "biome check --write .",
    "deploy:storybook": "bun run build-storybook",
    "dev": "storybook dev -p 6006",
    "format": "biome format --write .",
    "format:check": "biome check --formatter-enabled=false .",
    "format:fix": "biome format --write .",
    "lint:check": "biome lint .",
    "lint:fix": "biome lint --write .",
    "pre:install": "bun update --latest --recursive && bun install",
    "prepare": "husky",
    "prepare:deploy": "bun run check:fix && bun run build",
    "prepublishOnly": "node -e \"const fs = require('fs'); if (!fs.existsSync('dist')) { console.error('ERROR: dist directory not found. Run \\\"bun run build\\\" first.'); process.exit(1); }\"",
    "storybook": "storybook dev -p 6006",
    "test": "bun test",
    "test:coverage": "bunx vitest run --coverage",
    "typecheck": "tsc --noEmit"
  },
  "sideEffects": [
    "**/*.css",
    "**/*.scss"
  ],
  "type": "module",
  "types": "./dist/index.d.ts",
  "version": "0.4.0"
}
